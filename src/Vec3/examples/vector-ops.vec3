// define vectors
let x = [1,2,3]
let y = [3,2,1]

// x[5] // error due to indexing

// vector addition
print(x + y)
// vector cross product
print(x X y)
// vector dot product
print(x .* y)

// To note: the length is encoded in the type, so the compiler can catch errors like this:
